#include<stdio.h>
int main()
{
	int a,b,c;
	printf("enter values of a,b and c \n");
	scanf("%d %d %d",&a,&b,&c);
	printf("\n a=%d",a);
	printf("\n b=%d",b);
	printf("\n c=%d",c);
	a=a+b+c;
	b=a-b-c;
	c=a-b-c;
	a=a-b-c;
	printf("\n after swapping their values are as below -");
	printf("\n a=%d",a);
	printf("\n b=%d",b);
	printf("\n c=%d",c);
return 0;		
}
